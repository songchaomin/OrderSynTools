    <select id="queryOrderByList" resultType="com.kuka.domain.SalOrder">
        select * from sal_order
        <where>
            out_order_code in
            <foreach collection="orderNos" index="index" item="item" separator="," open="(" close=")">
                #{item}
            </foreach>
        </where>
    </select>
</mapper>